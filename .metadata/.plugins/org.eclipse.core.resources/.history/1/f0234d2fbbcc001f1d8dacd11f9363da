package aulas;

public class Vector {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n[] = {1, 2, 3, 4};
		System.out.println("O tamanho do vetor n[] : " + n.length);
		for (int i = 0; i <= n.length -1; i++) {
			System.out.println("Na posicao "+i+" tem o valor "+n[i]);
		}
	}

}
